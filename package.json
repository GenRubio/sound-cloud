{
  "name": "sound-cloud-app",
  "productName": "SoundCloud",
  "version": "1.0.0",
  "description": "Download and play youtube musics",
  "useFrame": true,
  "backgroundColor": "#ccc",
  "main": "index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build-installer": "electron-builder",
    "watch": "sass --watch app/public/scss/app.scss app/public/css/app.css"
  },
  "build": {
    "appId": "cool-app",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "icon.ico",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "nsis": {
      "installerIcon": "icon.ico",
      "uninstallerIcon": "icon.ico",
      "uninstallDisplayName": "Yocomania Launcher",
      "license": "license.txt",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "keywords": [],
  "author": "Gen Rubio",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "cool_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@popperjs/core": "^2.10.2",
    "bootstrap": "^5.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.6.0",
    "js-datepicker": "^5.18.0",
    "node-php-server": "^0.1.2",
    "os-utils": "0.0.14",
    "popper.js": "^1.16.1",
    "toastr": "^2.1.4",
    "ytdl-core": "^4.9.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.51",
    "@electron-forge/maker-deb": "^6.0.0-beta.51",
    "@electron-forge/maker-rpm": "^6.0.0-beta.51",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.51",
    "@electron-forge/maker-zip": "^6.0.0-beta.51",
    "electron": "9.0.0",
    "electron-builder": "^22.9.1"
  }
}
